{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    
    
      "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Referrer-Policy", "value": "no-referrer" },
            { "key": "X-Frame-Options", "value": "ALLOWALL" },
            { "key": "Content-Security-Policy", "value": "frame-ancestors 'self' https://www.youtube.com" }
          ]
        }
      ]
    
  }
  
  